{
  "name": "schildpad-website",
  "version": "1.0.0",
  "description": "Website of Schildpad Launcher",
  "main": "tailwind.config.js",
  "scripts": {
    "start": "npx @11ty/eleventy --serve & npx tailwindcss -c src/tailwind.config.js -i src/input.css -o _site/style.css --watch",
    "build": "ELEVENTY_PRODUCTION=true npx @11ty/eleventy && NODE_ENV=production npx tailwindcss -c src/tailwind.config.js -i src/input.css -o _site/style.css --minify"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/joelpaulkoch/schildpad-website.git"
  },
  "author": "Joel Koch",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/joelpaulkoch/schildpad-website/issues"
  },
  "homepage": "https://github.com/joelpaulkoch/schildpad-website#readme",
  "devDependencies": {
    "@11ty/eleventy": "^2.0.1",
    "tailwindcss": "^3.3.2"
  }
}
